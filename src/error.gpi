set xlabel 'Workers/Tasks, N'

set ylabel 'Average Normalized Cost'

set yrange [0:1]

set key top right

set style data histogram
set style histogram cluster errorbars gap 2 lw 1
set style fill solid 0.3
set boxwidth 0.9

set terminal png size 800,600
set output 'bin/error.png'

plot for [i=2:8:2] 'obj/error.dat' using i:i+1:xtic(1) title columnheader(i) 

set output
